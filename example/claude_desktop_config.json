{
  "mcpServers": {
    "deepauto-intern-assignment": {
      "command": "uv",
      "args": [
        "--directory",
        "<absolute path to where main.py is>",
        "run",
        "mcp-server"
      ]
    },
    "deepauto-intern-assignment-docker": {
      "command": "docker",
      "args": [
        "run",
        "--pull",
        "always",
        "--rm",
        "-i",
        "--name",
        "deepauto-intern-assignment-docker-claude",
        "gjeodnd12165/deepauto-intern-assignment"
      ]
    }
  }
}